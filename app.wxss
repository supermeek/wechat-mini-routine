/**app.wxss**/
@import 'weui.wxss';
.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
}
/* 页面小标题 */
.page_title{
  background: #f8f8f8;
  padding:10rpx 20rpx;
  font-size:30rpx;
  color: #999;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
}
.iconImg{
  width: 30px;
  height: 30px;
  margin-right: 10px;
  vertical-align: bottom;
}